<blocks app="Snap! 10-dev, https://snap.berkeley.edu" version="2"><palette><category name="AI" color="64,64,64,1"/></palette><block-definition s="__init nst extension__" type="command" category="WebSockets" helper="true"><header/><code/><translations/><inputs/><script><block s="doSetVar"><l>__module__nst__</l><l>libraries/ai/init.js</l></block></script></block-definition><block-definition s="create image using NST - advanced: %&apos;advanced&apos; download: %&apos;download&apos;" type="command" category="AI"><header/><code/><translations/><inputs><input type="%b">false</input><input type="%b">false</input></inputs><script><block s="doApplyExtension"><l>nst_turn_on_progress()</l><list/></block><block s="doApplyExtension"><l>nst_create_image(isAdvanced, isDownloadable)</l><list><block var="advanced"/><block var="download"/></list></block><block s="doWaitUntil"><block s="reportApplyExtension"><l>nst_check_image_converted()</l><list/></block></block><block s="doApplyExtension"><l>nst_turn_off_progress()</l><list/></block></script></block-definition><block-definition s="use costume %&apos;name&apos; for %&apos;type&apos; image" type="command" category="AI"><header/><code/><translations/><inputs><input type="%cst" readonly="true"><options>ยง_costumesMenu</options></input><input type="%s" readonly="true"><options>base&#xD;style</options></input></inputs><script><block s="doApplyExtension"><l>nst_use_costume(name, type)</l><list><block var="name"/><block var="type"/></list></block></script></block-definition><block-definition s="use stage for %&apos;type&apos; image" type="command" category="AI"><header/><code/><translations/><inputs><input type="%s" readonly="true"><options>base&#xD;style</options></input></inputs><script><block s="doApplyExtension"><l>nst_use_stage(type)</l><list><block var="type"/></list></block></script></block-definition><block-definition s="save and switch to NST image" type="command" category="AI"><header/><code/><translations/><inputs/><script><block s="doApplyExtension"><l>nst_save_as_costume()</l></block></script></block-definition><block-definition s="switch to NST image" type="command" category="AI"><header/><code/><translations/><inputs/><script><block s="doApplyExtension"><l>nst_switch_to_result()</l></block></script></block-definition><block-definition s="clear %&apos;type&apos; image" type="command" category="AI"><header/><code/><translations/><inputs><input type="%s" readonly="true"><options>base&#xD;style</options></input></inputs><script><block s="doApplyExtension"><l>nst_clear_image(type)</l><list><block var="type"/></list></block></script></block-definition><block-definition s="clear NST created image" type="command" category="AI"><code/><translations/><inputs/><script><block s="doApplyExtension"><l>nst_clear_converted_image()</l></block></script></block-definition><block-definition s="set NST %&apos;param&apos; to %&apos;value&apos; %" type="command" category="AI"><header/><code/><translations/><inputs><input type="%s" readonly="true"><options>base image size&#xD;style image size&#xD;stylization ratio</options></input><input type="%n">50</input></inputs><script><block s="doApplyExtension"><l>nst_set_parameter(param, value)</l><list><block var="param"/><block var="value"/></list></block></script></block-definition><block-definition s="set NST mode to %&apos;value&apos;" type="command" category="AI"><header/><code/><translations/><inputs><input type="%s" readonly="true"><options>fast&#xD;high quality</options></input></inputs><script><block s="doApplyExtension"><l>nst_set_mode(value)</l><list><block var="value"/></list></block></script></block-definition><block-definition s="get NST %&apos;param&apos;" type="reporter" category="AI"><header/><code/><translations/><inputs><input type="%s" readonly="true"><options>base image size&#xD;style image size&#xD;stylization ratio</options></input></inputs><script><block s="doApplyExtension"><l>nst_get_parameter(param)</l><list><block var="param"/></list></block></script></block-definition><block-definition s="get NST mode" type="reporter" category="AI"><header/><code/><translations/><inputs/><script><block s="doApplyExtension"><l>nst_get_mode()</l></block></script></block-definition><block-definition s="was %&apos;type&apos; image set" type="reporter" category="AI"><header/><code/><translations/><inputs><input type="%s" readonly="true"><options>base&#xD;style</options></input></inputs><script><block s="doApplyExtension"><l>nst_check_image_generated(type)</l><list><block var="type"/></list></block></script></block-definition><block-definition s="was NST image created" type="reporter" category="AI"><header/><code/><translations/><inputs/><script><block s="doReport"><block s="reportApplyExtension"><l>nst_check_image_converted()</l><list/></block></block></script></block-definition><block-definition s="switch to NST image when finished" type="command" category="AI"><header/><code/><translations/><inputs/><script><block s="doWaitUntil"><block s="reportApplyExtension"><l>nst_check_image_converted()</l></block></block><block s="doApplyExtension"><l>nst_switch_to_result()</l><list/></block></script></block-definition><block-definition s="save and switch to NST image when finished" type="command" category="AI"><header/><code/><translations/><inputs/><script><block s="doWaitUntil"><block s="reportApplyExtension"><l>nst_check_image_converted()</l></block></block><block s="doApplyExtension"><l>nst_save_as_costume()</l><list/></block></script></block-definition><block-definition s="stamp NST image when finished" type="command" category="AI"><header/><code/><translations/><inputs/><script><block s="doWaitUntil"><block s="reportApplyExtension"><l>nst_check_image_converted()</l></block></block><block s="doApplyExtension"><l>nst_switch_to_result()</l><list/></block><block s="doStamp"/></script></block-definition><variables><variable name="__module__nst__" hidden="true"><l>libraries/ai/init.js</l></variable></variables></blocks>
